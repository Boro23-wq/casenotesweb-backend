{
  "version": 2,
  "env": {
    "DATABASE_URL": "@DATABASE_URL",
    "ACCESS_KEY_ID_AWS": "@ACCESS_KEY_ID_AWS",
    "SECRET_ACCESS_KEY_AWS": "@SECRET_ACCESS_KEY_AWS"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
